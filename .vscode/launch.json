{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Next.js Server",
        "type": "pwa-node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "cwd": "${workspaceFolder}",
        "skipFiles": ["<node_internals>/**","**/node_modules/**"]
      },
      {
        "name": "Next.js Client (Chrome)",
        "type": "pwa-chrome",
        "request": "launch",
        "url": "http://localhost:9002",
        "webRoot": "${workspaceFolder}",
        "skipFiles": ["<node_internals>/**","**/node_modules/**"]
      }
    ],
    "compounds": [
      {
        "name": "Next.js Full Stack",
        "configurations": ["Next.js Server", "Next.js Client (Chrome)"]
      }
    ]
  }
  